import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		System.out.println("Printing the Sum of series....");
		System.out.println("1 + 1/1 + 1/2 + 1/3 + 1/4 + 1/5");
		float sum =0;
		
		for (float i = 1; i<=5; i++){
		    sum = sum + 1/i;
		}
		
		System.out.println("\nSum of the series is "+sum);
		
	}
}
